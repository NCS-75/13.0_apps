<?xml version="1.0"?>
<odoo>
    <data noupdate="1">
        <record model="ir.cron" id="export_contacts_cron">
            <field name="name">Export Google Contacts</field>
            <field name="model_id" ref="model_google_contacts"/>
            <field name="state">code</field>
            <field name="code">model.export_contacts_cron()</field>
            <field name="active" eval="True"/>
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
        </record>

        <record model="ir.cron" id="import_contacts_cron">
            <field name="name">Import Google Contacts</field>
            <field name="model_id" ref="model_google_contacts"/>
            <field name="state">code</field>
            <field name="code">model.import_contacts_cron()</field>
            <field name="active" eval="True"/>
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
        </record>
    </data>
</odoo>